{
  "manifest_version": 2,
  "name": "Google Minus AI",
  "description": "Adds a search engine that disables Google Gemini content in the search results",
  "version": "1.0.0",
  "author": "Vlad-Andrei Popescu",
  "homepage_url": "https://github.com/vapopescu/google_minus_ai",
  "icons": {
    "16": "img/icon.svg",
    "32": "img/icon.svg",
    "48": "img/icon.svg",
    "64": "img/icon.svg",
    "96": "img/icon.svg",
    "128": "img/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "@google-minus-ai"
    },
    "gecko_android": {}
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Google Minus AI",
      "search_url": "https://www.google.com/search?q={searchTerms}+-ai",
      "keyword": "@gmai",
      "favicon_url": "img/favicon.ico",
      "is_default": true,
      "encoding": "UTF-8"
    }
  }
}